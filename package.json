{
  "name": "aquanode",
  "description": "aquanode",
  "version": "1.1.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "server:test": "echo \"Error: no test specified\" && exit 1",
    "server:build": "tsc -p server",
    "server:serve": "node server/dist/src/server.js",
    "server:start": "npm run server:build && forever start server/dist/src/server.js",
    "server:start:dev": "cross-env NODE_ENV=development ./node_modules/nodemon/bin/nodemon.js -e ts -w ./server/src -x \"npm run server:build && npm run server:serve\"",
    "server:lint": "tslint server/src/**/*.ts --fix --project server",
    "client:build": "ng build",
    "client:start": "ng serve",
    "client:test": "ng test",
    "client:pree2e": "webdriver-manager update --standalone false --gecko false",
    "client:e2e": "protractor",
    "client:lint": "tslint client/src/**/*.ts --fix --project client"
  },
  "dependencies": {
    "@angular/animations": "^5.2.7",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.7",
    "@angular/compiler": "^5.2.7",
    "@angular/core": "^5.2.7",
    "@angular/forms": "^5.2.7",
    "@angular/http": "^5.2.7",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.7",
    "@angular/platform-browser-dynamic": "^5.2.7",
    "@angular/platform-server": "^5.2.7",
    "@angular/router": "^5.2.7",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^5.1.0",
    "@ngrx/store": "^5.1.0",
    "agenda": "^1.0.3",
    "body-parser": "^1.17.2",
    "core-js": "^2.4.1",
    "cors": "^2.8.4",
    "errorhandler": "^1.5.0",
    "express": "^4.15.4",
    "normalize.css": "^8.0.0",
    "rxjs": "^5.0.1",
    "tinygradient": "^0.3.1",
    "ts-helpers": "^1.1.1",
    "winston": "^2.4.0",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.2",
    "@angular/compiler-cli": "^5.2.7",
    "@types/agenda": "^1.0.2",
    "@types/body-parser": "^1.16.4",
    "@types/cors": "^2.8.3",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.11.1",
    "@types/jasmine": "^2.8.6",
